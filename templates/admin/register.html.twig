        <div class="row">
            <div class="col m6 offset m3">
                    {{form_start(form)}}


                   
                {% if email is defined %}
                {{form_label(form.email)}}
                {{form_widget(form.email,{'attr': {'value': email}})}}
                {% else %}
                {{form_row(form.email)}}
                {% endif %}
                
                
                    
                    
                    {{form_row(form.password)}}
                    {{form_row(form.Inscrire)}}
                    
                  
                    {{form_end(form)}}
                  
                   
           
            </div>
            
        </div>
        <a  class="float-right" href="{{path('app_login')}}">Connexion</a>
    
    </div>
</div>

